          </view>
        </view>
      </swiper-item>

      <swiper-item>

        <view class="firstView" catchtouchmove="setTouchMove" catchtouchend='setCoordinates'
          style="left: {{left||'89%'}};top: {{top||'65%'}};">
          <view>
            <image class='kehuImg' bindtap='makephone' src="pj.png"></image>
          </view>
          <text class="pj">评价</text>
        </view>

        <view class="container">
