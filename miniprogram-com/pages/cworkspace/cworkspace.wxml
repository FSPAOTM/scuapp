      <swiper-item>
        <view class="post" wx:for="{{workinfo1}}" wx:for-item="item" wx:for-index="index">
          <view class='nav-class' bindtap="yibaoming" data-index="{{index}}">
            <view class="postt">
              <view style="font-size:12px" class="jobb">岗位：{{item.post}}</view>
            </view>
            <view class="baoming">
              <view style="font-size:12px" class='xiangqing'>报名者：{{item.user}}</view>
            </view>
          </view>
        </view>
      </swiper-item>


      <swiper-item>
        <view class="post" wx:for="{{workinfo2}}" wx:for-item="item" wx:for-index="index">
          <view class='nav-class' bindtap="weitongguo" data-index="{{index}}">
            <view class="postt">
              <view style="font-size:12px" class="jobb">岗位：{{item.post}}</view>
            </view>
            <view class="baoming">
              <view style="font-size:12px" class='xiangqing'>报名者：{{item.user}}</view>
            </view>
          </view>
        </view>
      </swiper-item>

      <swiper-item>

        <view class="post" wx:for="{{workinfo3}}" wx:for-item="item" wx:for-index="index">
          <view class='item_container' >
          <image style="margin-top:2px;" class='select_icon'
                src="{{item.isSelected?'isselected.png':'unselected.png'}}" bindtap='itemSelected' data-index="{{index}}"></image>
            <view class="postt">
              <view style="font-size:12px" class="jobb">岗位：{{item.post}}</view>
            </view>
            <view class="baoming">
              <view style="font-size:12px" class='xiangqing'>报名者：{{item.user}}</view>
            </view>
          </view>
        </view>

        <!--<view class='h1' wx:for="{{details}}" wx:key="key" bindtap='itemSelected' data-index='{{index}}'>
