<template name="workinfo">
  <view class='xiangqing_info' style=" padding-left:30rpx;" bindtap="navigateTo">
    <text class='zi'>{{item.type}}</text>
    <text class='title'>{{item.title}}</text>

    <view class='house' style=" padding-left:30rpx;">{{item.amount}}|{{item.place}}</view>
    <view class='prix'>
      <text class='prix1' style=" padding-left:30rpx;">{{item.salary}}</text>
      <text>{{item.depart}}</text>

      <view class='arrow' bindtap="modalput"></view>
    </view>
  </view>
</template>